{
  "name": "tanstack-convex-selfhosted-starter-template",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "dev": "npx convex dev --once && concurrently -r npm:dev:web npm:dev:db",
    "dev:web": "vite dev",
    "dev:db": "convex dev --run todos:seed",
    "build": "vite build && tsc --noEmit",
    "preview": "vite preview",
    "start": "pnpx srvx --prod -s ../client dist/server/server.js"
  },
  "dependencies": {
    "@convex-dev/react-query": "0.0.0-alpha.8",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-query-devtools": "^5.91.2",
    "@tanstack/react-router": "^1.144.0",
    "@tanstack/react-router-devtools": "^1.144.0",
    "@tanstack/react-router-ssr-query": "^1.144.0",
    "@tanstack/react-start": "^1.145.3",
    "concurrently": "^8.2.2",
    "convex": "^1.31.2",
    "ky": "^1.14.2",
    "msw": "^2.12.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hot-toast": "^2.6.0",
    "redaxios": "^0.5.1",
    "tailwind-merge": "^2.6.0",
    "tiny-invariant": "^1.3.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "22.10.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
